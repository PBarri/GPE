<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
		<locale-config>
			<default-locale>es</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>messages</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>
	<component>
		<component-type>com.awg.gpe.components.Card</component-type>
		<component-class>com.awg.gpe.web.primefaces.components.Card</component-class>
	</component>
	<component>
		<component-type>com.awg.gpe.components.Cards</component-type>
		<component-class>com.awg.gpe.web.primefaces.components.Cards</component-class>
	</component>
	<component>
		<component-type>com.awg.gpe.components.CardList</component-type>
		<component-class>com.awg.gpe.web.primefaces.components.CardList</component-class>
	</component>
	<render-kit>
		<renderer>
			<component-family>org.primefaces.component</component-family>
			<renderer-type>org.primefaces.component.DataTableRenderer</renderer-type>
			<renderer-class>com.awg.gpe.web.primefaces.renderers.GPEDataTableRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.component</component-family>
			<renderer-type>org.primefaces.component.WizardRenderer</renderer-type>
			<renderer-class>com.awg.gpe.web.primefaces.renderers.GPEWizardRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.component</component-family>
			<renderer-type>com.awg.gpe.renderers.CardRenderer</renderer-type>
			<renderer-class>com.awg.gpe.web.primefaces.renderers.CardRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.component</component-family>
			<renderer-type>com.awg.gpe.renderers.CardListRenderer</renderer-type>
			<renderer-class>com.awg.gpe.web.primefaces.renderers.CardListRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.component</component-family>
			<renderer-type>com.awg.gpe.renderers.CardsRenderer</renderer-type>
			<renderer-class>com.awg.gpe.web.primefaces.renderers.CardsRenderer</renderer-class>
		</renderer>
	</render-kit>
</faces-config>